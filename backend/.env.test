# Test environment with secure values for validation testing
NODE_ENV=development
PORT=9000
DISABLE_AUTH=false

# Frontend URL for CORS
FRONTEND_URL=http://localhost:5173

# JWT Configuration - SECURE VALUES
JWT_SECRET=super_secure_jwt_secret_that_is_definitely_long_enough_for_production
JWT_EXPIRY=24h

# Session Secret - SECURE VALUE
SESSION_SECRET=another_super_secure_session_secret_for_testing_validation

# Application Info
APP_VERSION=2.1.0-nexus-genesis

# Authentication - SECURE PASSWORDS
ADMIN_PASSWORD=SecureAdmin123!@#
ADMIN_EMAIL=admin@nexus-genesis.com
OPERATOR_PASSWORD=SecureOperator456$%^
OPERATOR_EMAIL=operator@nexus-genesis.com

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434

# System Monitoring
METRICS_COLLECTION_INTERVAL=30000

# Logging
LOG_LEVEL=info

# Security
MAX_LOGIN_ATTEMPTS=5
LOGIN_LOCKOUT_DURATION=900000

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX_REQUESTS=100

# Production Configuration
DATABASE_PATH=./data/nexus-genesis.db
BACKUP_INTERVAL=3600000
MAX_CONCURRENT_AGENTS=25
SYSTEM_MONITORING_ENABLED=true