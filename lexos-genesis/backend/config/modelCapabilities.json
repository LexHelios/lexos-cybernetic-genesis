  "routing_rules": {
    "task_patterns": {
      "coding": {
        "patterns": ["code", "program", "function", "debug", "implement", "fix", "error", "syntax", "api", "class", "method"],
        "preferred_models": ["qwen2.5-coder:7b", "mixtral:8x7b"],
        "fallback": "mistral:7b"
      },
      "reasoning": {
        "patterns": ["explain", "why", "how", "analyze", "reason", "logic", "proof", "solve", "calculate", "deduce"],
        "preferred_models": ["deepseek-r1:latest", "r1-unrestricted:latest", "mixtral:8x7b"],
        "fallback": "llama3.3:70b"
      },
      "creative": {
        "patterns": ["write", "story", "poem", "creative", "imagine", "fiction", "narrative", "describe", "compose"],
        "preferred_models": ["llama3.3:70b", "mistral:7b"],
        "fallback": "mixtral:8x7b"
      },
      "research": {
        "patterns": ["research", "study", "analyze", "data", "statistics", "scientific", "evidence", "paper", "journal"],
        "preferred_models": ["gemma3n-unrestricted:latest", "deepseek-r1:latest", "gemma2:9b"],
        "fallback": "mixtral:8x7b"
      },
      "simple": {
        "patterns": ["what is", "who is", "when", "where", "define", "meaning", "simple", "basic"],
        "preferred_models": ["phi3:mini", "mistral:7b"],
        "fallback": "gemma2:9b"
